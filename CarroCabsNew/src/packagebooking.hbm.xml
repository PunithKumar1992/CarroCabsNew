<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.appfone.carro.pojo.Packagebooking" table="packagebooking">  
    <id name="packbook_id">  
     <generator class="increment"></generator>  
    </id>  
            
   <property name="vehicle"></property>
   <property name="packageselected"></property>
   <property name="price"></property>
   <property name="gst"></property>
   <property name="total"></property>
   <property name="cust_name"></property>
   <property name="cust_phone"></property>
   <property name="cust_email"></property>
   <property name="cust_address"></property>
   <property name="date"></property>
   <property name="pickup_point"></property>
            
  </class>  
  
  </hibernate-mapping>