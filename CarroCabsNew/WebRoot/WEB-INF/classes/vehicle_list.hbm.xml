<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.appfone.carro.pojo.Vehicle_list" table="vehicle_list">  
    <id name="v_id">  
     <generator class="increment"></generator>  
    </id>  
            
   <property name="vehicle"></property>
   <property name="km_per_day"></property>
   <property name="price_per_km"></property>
   <property name="driverbata"></property>
   <property name="v_image"></property>
            
  </class>  
            
 </hibernate-mapping>  